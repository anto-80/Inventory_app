<section class="warehouses">
    {{!-- what is happening below? Where does {{image}}, {{id}}, {{name}} come from? --}}
    {{#each warehouses}} 
        <article class="warehouse">
            <h2>{{this.name}}</h2>
            <img id="myImg" src="{{this.image}}" alt="snow" style />
