<section class="warehouses row">
    {{#each warehouses}} 
        <article class="warehouse col-md-4">
            <h2>{{this.name}}</h2>
            <img id="myImg" src="{{this.image}}" alt="snow" style />
            <br />
            <a class="myBtn" href="/warehouses/{{id}}"><button class=" btn-sm btn-primary">{{name}}</button></a>		
        </article>
    {{/each}}
</section> 